[user]
    ui = color
    name = whoami
    email = whose@mail.ai
[push]
    default = current
[alias]
    cl = clone
    pl = pull
    ps = push
    psf = push --force
    st = status
    df = diff
    dfc = diff --cached
    cm = commit -m
    cmm = "!git commit -m \"`commit-message`\""
    ad = add
    br = branch
    ch = checkout
    cp = cherry-pick
    graph = log --graph --abbrev-commit --pretty=oneline
    lg = log
    gatto = pull up master --rebase  # ガッとする
[color]
    ui = true
[init]
    templatedir = ~/.dots/git/template
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[core]
    excludesfile = ~/.gitignore
